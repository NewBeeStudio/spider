        <script type="text/javascript">
    var tokenCookieName = "knowbox_teacherToken";
    var infoCookieName = "knowbox_teacherInfo";
    var classListCookieName = "knowbox_teacherClassList";
    var jiaocaiCookieName = "knowbox_teacherJiaocai";
    var assignmentCookieName = "knowbox_teacherAssignment";
    function ReadCookie1(name) {
        var mycookie = document.cookie;
        var start1 = mycookie.indexOf(name + "=");
        if (start1 == -1)
            return "";
        else {
            start = mycookie.indexOf("=", start1) + 1;
            var end = mycookie.indexOf(";", start);
            if (end == -1) {
                end = mycookie.length;
            }
            var value = unescape(mycookie.substring(start, end));
            return value;
        }
    }

    function SetCookie1(name, value, seconds) {
        // seconds
        var exp = new Date();
        exp.setTime(exp.getTime() + seconds * 1000);
        document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString() + ";path=/";
    }

    if (ReadCookie1(tokenCookieName) == "") {
        window.location.href = "Login.aspx";
    }

    function logout() {
        var teacherInfo = ReadCookie(infoCookieName);
        SetCookie1(tokenCookieName, "", -1);
        SetCookie1(infoCookieName, "", -1);
        SetCookie1(classListCookieName, "", -1);
        SetCookie1(jiaocaiCookieName, "", -1);
        SetCookie1(assignmentCookieName, "", -1);
        SetCookie1("EDU_COOKIE_TEACHER", "", -1);

        var logoutUrl = ReadCookie("logoutUrl");
        if (logoutUrl!="") {
            window.location.href = logoutUrl;
            SetCookie1("logoutUrl", "", -1);
        }
        else {
            window.location.href = "Login.aspx";
        }
    }

    function checkResponse(data) {
        if (data.code == "20001") {
            logout();
        }
    }
</script>
