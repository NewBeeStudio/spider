<script id="tmpl_section" type="text/x-jquery-tmpl">
    <li class="J_section">
        <h5><a href="javascript:void(0);" rel="${courseSectionID}" level="1" type="0">${sectionName}</a></h5>
        <dl>
            {{each(i,sec) list}}
            
            {{if sec.questionNum > 0}}
            <dd>
            {{else}}
            <dd class="noques">
            {{/if}}
                <a href="javascript:void(0);" class="tfrist" rel="${sec.courseSectionID}" level="2" type="0" title="${sec.sectionName}"><div class="name">${sec.sectionName}</div><div class="questionnum">${sec.questionNum}</div><div style="clear:both"></div></a>
                <div class="thrednav">
                    <div class="yhdsaj">
                        {{each(i,sec3) sec.list}}
                            {{if sec3.questionNum > 0}}
                            <a href="javascript:void(0);" rel="${sec3.courseSectionID}" level="3" type="0" title="${sec3.sectionName}">
                            {{else}}
                            <a href="javascript:void(0);" rel="${sec3.courseSectionID}" level="3" type="0" title="${sec3.sectionName}" class="noques">
                            {{/if}}
                            <p>${sec3.sectionName}</p>
                            <div class="questionnum" style="padding:9px 4px 9px 0;">${sec3.questionNum}</div>
                        </a>
                        {{/each}}                       
                    </div>
                </div>
            </dd>
            {{/each}}           
        </dl>
    </li>
</script>

<script id="knowledge_tmpl" type="text/x-jquery-tmpl">
    <li class="J_section">
        <h5><a href="javascript:void(0);" rel="${knowID}" level="1" type="1">${knowledgeName}</a></h5>
        <dl>
            {{each(i,know) list}}
            
            <dd>
                
                <a href="javascript:void(0);" class="tfrist" rel="${know.knowID}" level="2" type="1" title="${know.knowledgeName}"><div class="name">${know.knowledgeName}</div><div class="questionnum">${know.questionNum}</div><div style="clear:both"></div></a>
                <div class="thrednav">
                    <div class="yhdsaj">
                        {{each(i,know3) know.list}}
                        <a href="javascript:void(0);" rel="${know3.knowID}" level="3" type="1">
                            <p>${know3.knowledgeName}</p>
                            <div class="questionnum" style="padding:9px 4px 9px 0;">${know3.questionNum}</div>
                        </a>
                        {{/each}}                       
                    </div>
                </div>
            </dd>
